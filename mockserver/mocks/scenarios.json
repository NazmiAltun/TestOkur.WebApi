{
  "port": 8080,
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "*"
  },
  "routes": {
    "/connect/token": {
      "post": "token.js"
    },
	"/api/v1/license-types": {
      "options": 204,
      "get": "license-types.json"
    },
	"/api/v1/cities": {
      "options": 204,
      "get": "cities.json"
    },
    "/api/v1/users": {
      "options": 204,
      "get": "users.json"
    },
    "/api/v1/users/create": {
      "options": 204,
      "post": 204
    },
    "/api/v1/users/:id": {
      "options": 204,
      "delete": 204
    },
    "/api/v1/users/update": {
      "options": 204,
      "post": 204
    },
    "/api/v1/users/activate": {
      "post": 200
    },
    "/api/v1/users/extend:": {
      "post": 200
    },
    "/api/v1/users/generate-password-reset-token": {
      "post": "password-reset-token.html"
    },
    "/api/v1/settings/appsettings": {
      "get": "appsettings.json"
    },
    "/Templates/User_New_User_Mail.html": {
      "get": "User_New_User_Mail.html"
    },
    "/Templates/Admin_New_User_Mail.html": {
      "get": "Admin_New_User_Mail.html"
    },
    "/.well-known/openid-configuration": {
      "get": 200
    },
    "/hc": {
      "get": "hc.json"
    },
    "/captcha" :{
      "get" : "captcha.js"
    },
    "/captcha/hc": {
      "get": 200
    }
  }
}
