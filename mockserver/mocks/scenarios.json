{
  "port": 8080,
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "*"
  },
	"routes": {
		"/connect/token": {
			"post": "token.js"
		},
		"/account/api/users": {
			"options": 204,
			"get": "users.json"
		},
		"/account/create": {
			"options": 204,
			"post": 204
		},
		"/account/:id": {
			"options": 204,
			"delete": 204
		},
		"/account/update": {
			"options": 204,
			"post": 204
		},
		"/account/activate": {
			"post": 200
		},
		"/account/generate-password-reset-token": {
			"post": "password-reset-token.html"
		},
    "/api/v1/settings/appsettings": {
			"get": "appsettings.json"
		},
		"/Templates/User_New_User_Mail.html": {
			"get": "User_New_User_Mail.html"
		},
		"/Templates/Admin_New_User_Mail.html": {
			"get": "Admin_New_User_Mail.html"
		},
		"/.well-known/openid-configuration": {
			"get": 200
		},
		"/hc": {
			"get": "hc.json"
		}
	}
}
